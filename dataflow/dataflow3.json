{
	"name": "dataflow3",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "AzureSqlTable2",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "assert1"
				},
				{
					"name": "select1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          city_id as integer,",
				"          city as string,",
				"          state_id as integer,",
				"          status_id as integer,",
				"          created_date as date,",
				"          created_by as integer,",
				"          updated_date as date,",
				"          updated_by as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> source1",
				"source1 assert(expectTrue(state_id == 1 , false, 'assert1', state_id == 1 ),",
				"     expectTrue(state_id == 2, false, 'assert2', state_id == 2)) ~> assert1",
				"assert1 select(mapColumn(",
				"          city_id,",
				"          city,",
				"          state_id,",
				"          status_id,",
				"          created_date,",
				"          created_by,",
				"          updated_date,",
				"          updated_by",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1"
			]
		}
	}
}