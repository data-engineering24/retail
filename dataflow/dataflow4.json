{
	"name": "dataflow4",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "AzureSqlTable2",
						"type": "DatasetReference"
					},
					"name": "source"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "alterRow1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          CustomersNames as string,",
				"          Employee_names as string,",
				"          Payment_id as integer,",
				"          isactive as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> source",
				"source alterRow(deleteIf(ERROR_FUNCTION('isactive=__SINGLE_QUOTE__1__SINGLE_QUOTE__'))) ~> alterRow1"
			]
		}
	}
}